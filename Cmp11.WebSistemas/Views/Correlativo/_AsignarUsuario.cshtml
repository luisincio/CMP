@model Cmp02.Entities.Tb_Correlativos

@using (Ajax.BeginForm("Asociacion", "Correlativo", new AjaxOptions { UpdateTargetId = "divUsuario", HttpMethod = "POST", InsertionMode = InsertionMode.Replace }, new { @id = "frmEditarUsuario" }))
{
    <div id="divUsuario">
        @if (Model.Opcional == "OK")
        {
            <script>
                modal.modal("hide");
                $("#frmParametro").submit();
            </script>
        }
        else
        {
            @Html.Partial("_AsignarUsuario2", Model)
        }
    </div>
}
